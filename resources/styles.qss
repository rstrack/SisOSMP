*/

QTextEdit{
    font-size: 13px;
}

QDateEdit{
    border-radius: 2px;
    border:1px solid gray;
    background-color:white;
    font-size: 13px;
}

QGroupBox{
    font-size: 14px;
    
}

QScrollArea{
    border: none;
}

QTableView{
    font-size: 12px;
    selection-background-color: #1276B9
}

QCheckBox{
    font-size: 14px;
    font-weight: bold;
    height: 20px;
    border-radius: 3px;
    background-color: #3498DB;
    color:#FFFFFF;
    padding: 2px 5px 2px 5px;
}

QCheckBox::indicator{
    width: 16px;
    height: 16px; 
}

QHeaderView::section{
    background-color: #F8F8F8;
    font-size: 13px;
}